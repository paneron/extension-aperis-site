    "@types/react": "17.0.53",
    "dagre": "^0.8.5",
    "immutability-helper": "^3.1.1",
    "react": "^17.0.2",
    "react-flow-renderer": "9.6.4",
    "typescript": "~4.6.0"
  },
  "license": "MIT"
